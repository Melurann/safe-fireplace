{
  "name": "safe-fireplace",
  "description": "Sveltekit and typesafe-i18n project for SAFE-Fireplace team",
  "version": "1.0.0",
  "author": "melurann",
  "homepage": "https://safe-fireplace.vercel.app/",
  "repository": {
    "type": "git",
    "url": "https://github.com/Melurann/SAFE-Fireplace"
  },
  "scripts": {
    "dev": "npm-run-all --parallel vite typesafe-i18n",
    "vite": "vite dev --open",
