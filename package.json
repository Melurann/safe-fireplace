    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.1",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "svelte": "^5.0.0",
    "svelte-check": "^3.4.5",
    "svelte-preprocess": "^6.0.3",
    "tslib": "^2.6.0",
    "typesafe-i18n": "^5.24.3",
    "typescript": "^5.7.3",
    "vite": "^6.1.0"
  },
  "type": "module",
  "dependencies": {
